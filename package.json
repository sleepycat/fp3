{
	"name": "remix-vite",
	"version": "0.1.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"prepare": "panda codegen",
		"dev": "concurrently 'npm run dev:*'",
		"build": "react-router build",
		"dev:rr": "react-router dev --port 3000",
		"dev:css": "panda --watch",
		"lint": "eslint --fix --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"test": "vitest run --reporter=verbose",
		"extract": "lingui extract --clean",
		"compile": "lingui compile",
		"start": "react-router-serve ./build/server/index.js",
		"typecheck": "react-router typegen && tsc"
	},
	"dependencies": {
		"@lingui/core": "^5.3.1",
		"@lingui/detect-locale": "^5.3.1",
		"@lingui/macro": "^5.3.1",
		"@lingui/react": "^5.3.1",
		"@react-router/node": "^7.5.0",
		"@react-router/serve": "^7.5.0",
		"@std/fmt": "npm:@jsr/std__fmt@^1.0.6",
		"@std/http": "npm:@jsr/std__http@^1.0.14",
		"concurrently": "^9.1.2",
		"cookie": "^1.0.2",
		"intl-parse-accept-language": "^1.0.0",
		"isbot": "^4.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^7.5.0"
	},
	"devDependencies": {
		"@lingui/babel-plugin-lingui-macro": "^5.3.1",
		"@lingui/cli": "^5.3.1",
		"@lingui/conf": "^5.3.1",
		"@lingui/vite-plugin": "^5.3.1",
		"@pandacss/dev": "^0.53.3",
		"@react-router/dev": "^7.5.0",
		"@testing-library/react": "^16.3.0",
		"@types/accept-language-parser": "^1.5.6",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^8.38.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jsdom": "^26.0.0",
		"typescript": "^5.1.6",
		"vite": "^5.1.0",
		"vite-plugin-babel-macros": "^1.0.6",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^3.1.1"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
